{
  "name": "homebridge-platform-wemo",
  "displayName": "Homebridge Wemo",
  "version": "1.6.1",
  "author": "homebridge-plugins",
  "description": "A Wemo https://www.wemo.com plugin for Homebridge: https://github.com/homebridge/homebridge",
  "main": "dist/index.js",
  "scripts": {
    "developer": "DEBUG=honeywell-home homebridge -D -U `pwd` -P `pwd`",
    "lint": "eslint src/**.ts",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "license": "Apache-2.0",
  "keywords": [
    "homebridge-plugin",
    "wemo-client",
    "wemo"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/homebridge-plugins/homebridge-platform-wemo.git"
  },
  "bugs": {
    "url": "https://github.com/homebridge-plugins/homebridge-platform-wemo/issues"
  },
  "engines": {
    "node": ">=12.0.0",
    "homebridge": ">=1.1.2"
  },
  "files": [
    "README.md",
    "CHANGELOG.md",
    "config.schema.json",
    "package-lock.json",
    "package.json",
    "nodemon.json",
    "LICENSE",
    "index.js"
  ],
  "dependencies": {
    "wemo-client": "^0.15.0",
    "debug": "^4.1.1",
    "rxjs": "^6.6.2"
  },
  "devDependencies": {
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "homebridge": "^1.1.2",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
